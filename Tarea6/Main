        AlDepArt alu = new AlDepArt();
        System.out.println(alu);
        alu.setNombre("Alexis");
        alu.setArte("Musica");
        alu.setDeporte("Gimnasia");
        alu.ensayar("Guitarra");
        alu.entrenar();
        alu.presentarCompetencia("naucalli");

    }
}
